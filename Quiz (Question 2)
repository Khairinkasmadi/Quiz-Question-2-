public class Car {
	

	    String brand;
	    String car;
		
	   // First object default constructor
	   Car(){
		this.brand="Proton X70";
		this.car="Car is running";
		
	   }
		
	   //Parameterized constructor / constructor with parameter
	   Car(String n,int a){
		this.brand="Proton X70";
		this.car="Car is running";
				
	   }
	  
	   public static void main(String args[]){
		   Car obj1 = new Car();
		  
		   // resume1 obj2 = new resume1("Khairin", 22);
		   	
		System.out.println(obj1.car+" "+obj1.brand);
		
		
		
	   }
	}
